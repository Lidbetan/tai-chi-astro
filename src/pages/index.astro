	<div class="w-full h-full flex flex-col justify-center items-center">
		<div
			class="flex flex-row justify-center items-center animate-fade-in-up animate-delay-500 animate-duration-slower"
		>
			<Image
				src="/caracteres-chinos.png"
				width={200}
				height={600}
				alt="Caracteres-chinos"
			/>
			<span class="font-bold italic text-5xl">"Aqui va <br /> una frase"</span>
		</div>
	</div>
</Layout>
